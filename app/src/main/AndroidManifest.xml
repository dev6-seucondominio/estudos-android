<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.estudosapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
    >

        <activity android:name=".erp.MainActivity">
        </activity>

        <activity
            android:name=".erp.home.HomeActivity"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".erp.contadorPessoas.ContadorPessoasActivity"
            android:parentActivityName=".erp.MainActivity"
        >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>

        <activity
            android:name=".erp.lista.ListaActivity"
            android:parentActivityName=".erp.MainActivity"
        >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>

        <activity
                android:name=".erp.usandoSharedPreferences.UsandoSharedPreferencesActivity"
                android:parentActivityName=".erp.MainActivity"
        >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>

        <activity
            android:name=".erp.usandoSqLite.UsandoSqLiteActivity"
            android:parentActivityName=".erp.MainActivity"
            >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>

        <activity
            android:name=".erp.carregarImagens.CarregarImagensActivity"
            android:parentActivityName=".erp.MainActivity"
            >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>

        <activity
            android:name=".erp.calculadora.CalculadoraActivity"
            android:parentActivityName=".erp.MainActivity"
            >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".erp.MainActivity"/>
        </activity>
    </application>

</manifest>